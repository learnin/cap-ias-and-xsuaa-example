{
  "routes": [
    {
      "source": "^/actuator/health$",
      "destination": "srv-api",
      "csrfProtection": false,
      "authenticationType": "none"
    },
    {
      "source": "^/rest/.*",
      "destination": "srv-api",
      "csrfProtection": true,
      "authenticationType": "ias"
    },
    {
      "source": "^/odata/.*",
      "destination": "srv-api",
      "csrfProtection": true,
      "authenticationType": "ias"
    }
  ]
}
